{
"parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  # "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "rules": {
    "indent": [2, "tab"],
    "no-tabs": 0,
    "jsx-a11y/img-uses-alt": 0,
    "jsx-a11y/label-uses-for": 0,
    "jsx-a11y/mouse-events-map-to-key-events": 0,
    "jsx-a11y/no-hash-href": 0,
    "jsx-a11y/redundant-alt": 0,
    "jsx-a11y/valid-aria-role": 0,
    "max-len": 0,
    "react/jsx-indent-props": [0, "tab"],
    "react/jsx-indent": [0, "tab"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "linebreak-style": [
        "error",
        "unix"
    ],
    "quotes": [
        "error",
        "single"
    ],
    "semi": [
        "error",
        "always"
    ]
  }
}
